STARTPROGRAM  
STARTBLOCK  
VARIABLE    INT16   _Value, _Devid, _Remad, _Cycac, _Cycbc, _Cyccc;
INPUT   _Value  
1 + _Value ==> _Value
GOTO _Ivann
FOR 0 ==> _Cycac TO 32767 DO  
    _Value - 1 ==> _Devid 
    FOR 0 ==> _Cycbc TO 32767 DO  
        IF (_Devid >> 1)  
            _Value ==> _Remad  
            FOR 0 ==> _Cyccc TO 32767 DO  
                IF (_Remad >> _Devid - 1)  
                    _Remad - _Devid ==> _Remad  
                ELSE  
                    32768 ==> _Cyccc  
            0 ==> _Cyccc  
            IF (_Remad = 0)  
                32768 ==> _Cycbc      
        ELSE  
            32768 ==> _Cycbc  
    0 ==> _Cycbc  
    IF (_Devid = 1)  
        32768 ==> _Cycac 
    1 + _Value ==> _Value  
0 ==> _Cycac  
_Ivann:
OUTPUT _Value  
INPUT   _Value 
ENDBLOCK